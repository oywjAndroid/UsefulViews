<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShapeImageView">
        <!-- Shape type-->
        <attr name="shapeType" format="enum">
            <enum name="normal" value="0" />
            <enum name="circle" value="1" />
            <enum name="round" value="2" />
            <enum name="oval" value="3" />
        </attr>
        <!-- Whether the shape have strokes -->
        <attr name="stroke" format="boolean" />
        <!-- The width of the shape stroke.  -->
        <attr name="strokeWidth" format="integer" />
        <attr name="strokeColor" format="color" />
        <!-- The radius of the shape round.-->
        <attr name="roundRadius" format="integer" />
    </declare-styleable>
</resources>